//Dynamic world data - Canadian wildfire areas


//Call on Canada shapefile

var Canada = ee.FeatureCollection("FAO/GAUL/2015/level1")
.filter(ee.Filter.eq("ADM0_NAME", "Canada"));


//Subset to BC and Alberta
var ROI = Canada.filter(ee.Filter.inList('ADM1_CODE', [825, 826]));

Map.addLayer(ROI);

