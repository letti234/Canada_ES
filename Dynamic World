//Dynamic world data - Canadian wildfire areas


//Call on Canada shapefile

var Canada = ee.FeatureCollection("FAO/GAUL/2015/level1")
.filter(ee.Filter.eq("ADM0_NAME", "Canada"));


//Subset to BC and Alberta
var ROI = Canada.filter(ee.Filter.inList('ADM1_CODE', [825, 826]));

Map.addLayer(ROI);

//Call on wildfire data
//fires = ee.FeatureCollection()

//Subset wildfire data to BC and Alberta

//Call on Dynamic World clipped to ROI

var lulc = ee.ImageCollection("GOOGLE/DYNAMICWORLD/V1")
  .filterBounds(ROI)
  .map(function(image){return image.clip(ROI)}) ;
  

//demo tile
var tile = lulc.first();
Map.addLayer(tile);
Map.centerObject(tile, 5);
//Crop Dynamic World to BC and AL


//Decompose Dynamic World into annual images





